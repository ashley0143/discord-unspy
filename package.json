{
  "name": "discord",
  "version": "3.0.1",
  "description": "Discord whitout spyware",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build"
  },
  "repository": "https://github.com/iamashley0/discord-unspy/",
  "keywords": [
    "discord"
  ],
  "author": "Ashley",
  "license": "MIT",
  "dependencies": {
    "@cliqz/adblocker": "^1.23.7",
    "@cliqz/adblocker-electron": "^1.23.7",
    "electron-log": "^4.4.6",
    "electron-updater": "^5.0.1",
    "node-fetch": "^3.2.3"
  },
  "devDependencies": {
    "electron": "^18.0.4",
    "electron-builder": "^23.0.3"
  },
  "build": {
    "appId": "xyz.iamashley.discordunspy",
    "productName": "Discord Unspy",
    "publish": {
      "provider": "generic",
      "url": "https://updates.korbsstudio.com/discord-nospy/"
    },
    "win": {
      "target": "nsis",
      "asar": "false"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "darkModeSupport": true,
      "category": "public.app-category.social-networking"
    },
    "linux": {
      "target": [
        "snap",
        "AppImage"
      ],
      "maintainer": "xyz.iamashley.discordunspy",
      "category": "Network",
      "vendor": "Ashley",
      "description": "Discord whitout spyware"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "shortcutName": "Discord Unspy",
      "displayLanguageSelector": true,
      "oneClick": false,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": false
    }
  }
}
